public class DLListTester {

	public static void main(String[] args) 
	{
		DLList myDL = new DLList();
		myDL.addToEnd(new DNode(999));
		myDL.addToBeginning(new DNode(333));
		//myDL.addToBeginning(new DNode(222));
		//myDL.addToBeginning(new DNode(111));
		myDL.addAfter(111,666);
		//myDL.addBefore(111,1);
		//myDL.delete(4);
		
		myDL.printList();
		System.out.println("- - - - - - ");
		myDL.printListBackwards();
		
		System.out.println("**********");
		System.out.println("Head:");
		if(myDL.head!=null)
		{
			System.out.println(myDL.head.data);
		}
		System.out.println("Tail:");
		if(myDL.tail!=null)
		{
			System.out.println(myDL.tail.data);
		}
	}

}
