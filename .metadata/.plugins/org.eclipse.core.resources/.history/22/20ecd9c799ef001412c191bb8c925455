import java.util.ArrayList;


public class QueueOfStrings 
{
	private DLList<DNode> theQ; // enqueue to the end, dequeue from the beginning
	
	public QueueOfStrings()
	{
		theQ = new DLList<DNode>(); // creates an empty array list 
	}
	
	public void enqueue(String s)
	{
		theQ.add(s);  // adds to the end
	}
	
	public DNode dequeue()
	{
		if (isEmpty())
		{
			System.out.println("Cannot dequeue because queue is empty");
			return null;
		}
		else
		{
			System.out.println("Dequed: " + theQ.get(0));
			return theQ.remove(0); // remove from the beginning
		}
	}
	
	public boolean isEmpty()
	{
		return theQ.size() == 0;
	}

}