public class UseList {

	public static void main(String[] args) 
	{
		LinkedList list = new LinkedList(new Node());
		
		list.addToEnd(new Node("Add to end 1"));
		list.addToEnd(new Node("Add to end 2"));
		list.addToEnd(new Node("Add to end 3"));
		list.addToEnd(new Node("Add to end 4"));
		list.addToEnd(new Node("Add to end 5"));
		
		System.out.println("the length of the list is: " + list.length());


	}

}
