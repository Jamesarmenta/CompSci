public class DivideConquer 
{
	public static void main(String args[])
	{
		int[] list = {1, 2, 3, 4, 5, 6, 7, 8};
		
		rearrange(list);
		
		for(int i = 0; i < list.length; i++)
		{
			System.out.print(list[i] + " ");
		}
	}
	
	public static int[] rearrange(int[]list)
	{
		int[] temp = new temp[10];
		rearrange(list, temp, 0);
		return list;
	}
	
	public static int[] rearrange(int[] list, int[] temp, int i)
	{
		if (i == list.length)
		{
			return list;
		}
		else 
		{
			if (list[i]%2 == 0)
			{
				temp
			}
		}
		
		return list;
	}
}
