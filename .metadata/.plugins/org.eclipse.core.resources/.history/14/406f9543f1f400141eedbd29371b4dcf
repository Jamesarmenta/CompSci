
public class TestAdjList {

	public static void main(String[] args) 
	{
		
		// Graph to test undirected, unweighted edges:
		Graph g = new Graph(10);
		g.addVertex("Zero");
		g.addVertex("One");
		g.addVertex("Two");
		g.addVertex("Three");
		g.addVertex("Four");
		g.addVertex("Five");
		g.addVertex("Six");
		g.addVertex("Seven");
		g.addVertex("Eight");
		g.addVertex("Nine");

		g.addEdge(0,1);
		g.addEdge(0,3);
		g.addEdge(0,6);
		g.addEdge(0,7);
		g.addEdge(1,6);
		g.addEdge(1,5);
		g.addEdge(1,2);
		g.addEdge(2,3);
		g.addEdge(2,8);
		g.addEdge(2,4);
		g.addEdge(4,9);
		g.addEdge(8,9);
		
		String output = g.BFS(3);
		System.out.println("BFS starting at 3");
		System.out.println(output);
		
		output = g.DFS(3);
		System.out.println("\nDFS starting at 3");
		System.out.println(output);
		
		output = g.shortestPath(3);
		System.out.println("\nShortest paths starting at 3");
		System.out.println(output);
		
		// Create graph to test directed, unweighted edges:
		Graph g2 = new Graph(10);
		g2.addVertex("Zero");
		g2.addVertex("One");
		g2.addVertex("Two");
		g2.addVertex("Three");
		g2.addVertex("Four");
		g2.addVertex("Five");
		g2.addVertex("Six");
		g2.addVertex("Seven");
		g2.addVertex("Eight");
		g2.addVertex("Nine");

		g2.addDirectedEdge(0,1);
		g2.addDirectedEdge(0,3);
		g2.addDirectedEdge(0,6);
		g2.addDirectedEdge(0,7);
		g2.addDirectedEdge(1,6);
		g2.addDirectedEdge(1,5);
		g2.addDirectedEdge(1,2);
		g2.addDirectedEdge(2,3);
		g2.addDirectedEdge(2,8);
		g2.addDirectedEdge(2,4);
		g2.addDirectedEdge(4,9);
		g2.addDirectedEdge(8,9);

		output = g2.BFS(0);
		System.out.println("BFS starting at 0");
		System.out.println(output);
		
		output = g2.DFS(0);
		System.out.println("\nDFS starting at 0");
		System.out.println(output);
		
		// Create graph to test undirected, weighted edges:
		Graph g3 = new Graph(10);
		g3.addVertex("Zero");
		g3.addVertex("One");
		g3.addVertex("Two");
		g3.addVertex("Three");
		g3.addVertex("Four");
		g3.addVertex("Five");
		g3.addVertex("Six");
		g3.addVertex("Seven");
		g3.addVertex("Eight");
		g3.addVertex("Nine");

		g3.addEdgeWithWeight(0,1,5);
		g3.addEdgeWithWeight(0,3,7);
		g3.addEdgeWithWeight(0,6,9);
		g3.addEdgeWithWeight(0,7,6);
		g3.addEdgeWithWeight(1,6,3);
		g3.addEdgeWithWeight(1,5,9);
		g3.addEdgeWithWeight(1,2,2);
		g3.addEdgeWithWeight(2,5,6);
		g3.addEdgeWithWeight(2,8,7);
		g3.addEdgeWithWeight(2,4,9);
		g3.addEdgeWithWeight(3,5,6);
		g3.addEdgeWithWeight(4,9,3);
		g3.addEdgeWithWeight(8,9,3);
	
		// Create graph to test directed, weighted edges:
		Graph g4 = new Graph(10);
		g4.addVertex("Zero");
		g4.addVertex("One");
		g4.addVertex("Two");
		g4.addVertex("Three");
		g4.addVertex("Four");
		g4.addVertex("Five");
		g4.addVertex("Six");
		g4.addVertex("Seven");
		g4.addVertex("Eight");
		g4.addVertex("Nine");

		g4.addDirectedEdgeWithWeight(0,1,5);
		g4.addDirectedEdgeWithWeight(0,3,7);
		g4.addDirectedEdgeWithWeight(0,6,9);
		g4.addDirectedEdgeWithWeight(0,7,6);
		g4.addDirectedEdgeWithWeight(1,6,3);
		g4.addDirectedEdgeWithWeight(1,5,9);
		g4.addDirectedEdgeWithWeight(1,2,2);
		g4.addDirectedEdgeWithWeight(2,5,6);
		g4.addDirectedEdgeWithWeight(2,8,7);
		g4.addDirectedEdgeWithWeight(2,4,9);
		g4.addDirectedEdgeWithWeight(3,5,6);
		g4.addDirectedEdgeWithWeight(4,9,3);
		g4.addDirectedEdgeWithWeight(8,9,3);
		
		output = g4.Dijkstras(0);
		System.out.println("\nDijkstra's algorithm starting at 0");
		System.out.println(output);
		
		
	}

}
